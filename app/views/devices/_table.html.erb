<table class="table table-bordered">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Token</th>
      <th scope="col">Last Time Usage</th>
    </tr>
  </thead>
  <tbody>
    <% @devices.each do |device| %>
      <tr>
        <td><%= device.id %></td>
        <td><%= device.token %></td>
        <td class="text-success"><%= device.time_ago %></td>
      </tr>
    </tbody>
  <% end %>
</table>
